<Window x:Class="AniFile3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AniFile3"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel HorizontalAlignment="Left" LastChildFill="True" >
        <TextBox Name="_testLog" Height="200" HorizontalAlignment="Left" TextWrapping="Wrap" AcceptsReturn="True" VerticalAlignment="Top" DockPanel.Dock="Bottom"/>
        <WebBrowser Name="_testWebBrowser" HorizontalAlignment="Left" DockPanel.Dock="Top" />
    </DockPanel>
</Window>
